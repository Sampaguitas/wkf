{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "timotheedesurmont/wkf-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "server", "service_param", "service_stock"],
        "memory": 100
      },
      {
        "name": "client",
        "image": "timotheedesurmont/wkf-client",
        "hostname": "client",
        "essential": false,
        "memory": 300
      },
      {
        "name": "server",
        "image": "timotheedesurmont/wkf-server",
        "hostname": "server",
        "essential": false,
        "memory": 100
      },
      {
        "name": "service_param",
        "image": "timotheedesurmont/wkf-service_param",
        "hostname": "service_param",
        "essential": false,
        "memory": 100
      },
      {
        "name": "service_stock",
        "image": "timotheedesurmont/wkf-service_stock",
        "hostname": "service_stock",
        "essential": false,
        "memory": 100
      }
    ]
}